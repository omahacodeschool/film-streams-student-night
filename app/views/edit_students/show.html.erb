<div class="container p-rel">
	<h1 class = "text-center">Here are the results that were found for<%= @findStudent %></h1>
	<div class = "text-center"><a href="/edit_students">Go Back To Search</a></div>
	<table class="table">
		<thead>
			<tr>
				<th>
					Name
				</th>
				<th>
					Email
				</th>
				<th>
					
				</th>
				<th>

				</th>
			</tr>			
		</thead>
		<tbody>
			<div>
				<% @student.each do |student| %>
					<tr>
						<td>
							<%= student.name %>
						</td>
						<td>
							<%= student.email %>
						</td>
						<td>
							<a href="edit_students/edit?id=<%= student.id %>" class="btn custom-btn borderless">
								Edit <span class="glyphicon glyphicon-edit"></span>
						  </a>
						</td>
						<td>
							<a href="edit_students/destroy?id=<%= student.id %>" class= "btn custom-btn-dngr navbar-btn"> Delete</a>
						</td>
					</tr>
				<% end %>
			</div>	
			
		</tbody>
		
	</table>
</div>